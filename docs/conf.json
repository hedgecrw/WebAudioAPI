{
   "plugins": ["plugins/markdown"],
   "recurseDepth": 10,
   "source": {
      "includePattern": ".+\\.(js(doc|x)?|mjs)$",
      "excludePattern": "(^|\\/|\\\\)_",
      "exclude": ["library/webaudioapi/encoders/webm-muxer.min.mjs"]
   },
   "sourceType": "module",
   "tags": {
      "allowUnknownTags": false,
      "dictionaries": ["jsdoc", "closure"]
   },
   "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
         "includeDate": false,
         "outputSourceFiles": true,
         "staticFiles": {
            "include": [
               "./docs/static"
            ]
         }
      },
      "name": "WebAudioAPI"
   },
   "opts": {
      "template": "docs/templates/tui-jsdoc-template",
      "encoding": "utf8",
      "recurse": true
   }
}
